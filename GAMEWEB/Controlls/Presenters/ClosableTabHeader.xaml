<UserControl x:Class="GAMEWEB.Controlls.Presenters.ClosableTabHeader"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GAMEWEB.Controlls.Presenters"
             mc:Ignorable="d" Height="15">
    <StackPanel Orientation="Horizontal" Height="15">
        <Label x:Name="labelName" Content="TabNamey" VerticalAlignment="Center" Height="26" Margin="0, 0, 0, 0"/>
        <Button x:Name="buttonClose" VerticalAlignment="Center" Margin="2.5" Width="10" Height="10" Click="buttonClose_Click" Background="#FFFF9297"/>
    </StackPanel>
</UserControl>
